    "aws-cdk": "2.108.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.108.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
